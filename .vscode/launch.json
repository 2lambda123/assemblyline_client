{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "al_submit - smtp",
            "type": "python",
            "request": "launch",
            "program": "git/al_client/assemblyline_client/submit.py",
            "args": ["-m", "{\"dict\": {\"a\": 1, \"b\": true}, \"items\": [1,2,3,\"four\"],  \"subject\": \"FW: This is definitely an email subject.\", \"protocol\": \"SMTP\", \"replay\":\"requested\"}", "-j", "{\"ignore_cache\": true}", "Assemblyline 3.2 - Reference Manual.pdf"],
            "cwd": "${workspaceFolder}/../../",
            "console": "internalConsole"
        },
        {
            "name": "al_submit - http",
            "type": "python",
            "request": "launch",
            "program": "git/al_client/assemblyline_client/submit.py",
            "args": ["-a", "-n", "-m", "{\"web_url\": \"http://baddomain.com/badfile/Assemblyline 3.2 - Reference Manual.pdf\", \"protocol\": \"HTTP\"}", "-j", "{\"generate_alert\": true, \"services\": {\"resubmit\": [\"ExtraFeature\"]}}", "Assemblyline 3.2 - Reference Manual.pdf"],
            "cwd": "${workspaceFolder}/../../",
            "console": "internalConsole"
        }
    ]
}
